{
  "device": {
    "backend": {
      "type": "registry",
      "config": {
        "timeout": 5,
        "connect_timeout": 5,
        "retry_limit": 2,
        "proxy": {
          "url": "http://nydus-gar-proxy.nydus-system.svc.cluster.local:80",
          "fallback": false,
          "ping_url": "http://nydus-gar-proxy.nydus-system.svc.cluster.local:80/healthz",
          "check_interval": "5"
        }
      }
    },
    "cache": {
      "type": "blobcache"
    }
  },
  "mode": "direct",
  "digest_validate": false,
  "iostats_files": false,
  "enable_xattr": true,
  "amplify_io": 1048576,
  "fs_prefetch": {
    "enable": true,
    "threads_count": 8,
    "merging_size": 1048576,
    "prefetch_all": true
  }
}
